- breadcrumbs [\
  { home: true, url: home_path },\
  { title: 'Users' }\
]
.container-wrapper-user
  h1
    = t('.title')
  .user-index
    .row
      - @users.each do |user|
        = render user
  .pagination-container
    = paginate @users, theme: 'twitter-bootstrap-4',
                       pagination_class: "pagination-md"
